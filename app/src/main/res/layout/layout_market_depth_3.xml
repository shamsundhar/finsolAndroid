<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="@dimen/medium_margin"
    android:background="@drawable/bg_edittext_register"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TableRow android:paddingTop="@dimen/medium_margin"
        android:paddingBottom="@dimen/medium_margin">
        <TextView
            android:text="@string/text_volume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="?attr/title_color"
            android:fontFamily="@font/montserratsemibold"
            app:fontFamily="@font/montserratsemibold"
            android:gravity="center_horizontal"
            android:layout_weight="1"
            android:layout_column="1" />
        <TextView
            android:text="@string/text_open_interest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/montserratsemibold"
            app:fontFamily="@font/montserratsemibold"
            android:textColor="?attr/title_color"
            android:gravity="center_horizontal"
            android:layout_column="2" />
