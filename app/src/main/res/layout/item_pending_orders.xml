<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="@dimen/medium_margin"
    android:background="@drawable/bg_edittext_register"
    android:id="@+id/root"
    android:layout_marginBottom="@dimen/medium_margin">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/workQuantity"
        android:textSize="@dimen/small_text_size"
        app:layout_constraintTop_toTopOf="@id/status1"
        app:layout_constraintBottom_toBottomOf="@id/status1"
        app:layout_constraintStart_toStartOf="parent"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:textColor="?attr/text_light_color"
        android:text="Qty - 02"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/small_text_size"
        app:layout_constraintStart_toEndOf="@id/symbolName"
        android:layout_marginStart="@dimen/medium_margin"
        android:textStyle="bold"
        android:id="@+id/filledOrderedQuantity"
        app:layout_constraintTop_toTopOf="@id/status1"
        app:layout_constraintBottom_toBottomOf="@id/status1"
        android:textColor="?attr/title_color"
        android:text="18/20"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/status2"
        android:textSize="@dimen/small_text_size"
        android:background="@drawable/bg_aliceblue_round_corners"
        android:padding="@dimen/small_margin"
        android:paddingStart="@dimen/large_margin"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:paddingEnd="@dimen/large_margin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="?attr/text_box_color"
        tools:text="Buy"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/status1"
        android:textSize="@dimen/small_text_size"
        android:background="@drawable/bg_teagreen_round_corners"
        android:padding="@dimen/small_margin"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:paddingStart="@dimen/large_margin"
        android:paddingEnd="@dimen/large_margin"
        android:layout_marginRight="@dimen/medium_margin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/status2"
        android:textColor="?attr/text_box_color"
        android:text="Pending"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/symbolName"
        android:textSize="@dimen/medium_text_size"
        android:textColor="?attr/title_color"
        android:fontFamily="@font/montserratbold"
        app:fontFamily="@font/montserratbold"
        android:layout_marginTop="@dimen/medium_margin"
        android:layout_marginEnd="@dimen/medium_margin"
        app:layout_constraintTop_toBottomOf="@id/status1"
        app:layout_constraintStart_toStartOf="parent"
        android:textStyle="bold"
        android:text="Peugot"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/symbolPrice"
        android:text="Avg. 24563"
        android:textColor="?attr/title_color"
        android:fontFamily="@font/montserratbold"
        app:fontFamily="@font/montserratbold"
        android:textStyle="bold"
        android:textSize="@dimen/medium_text_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/symbolName"
        app:layout_constraintBottom_toBottomOf="@id/symbolName"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/symbolExchange"
        android:textSize="@dimen/small_text_size"
        android:layout_marginTop="@dimen/medium_margin"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:textColor="?attr/text_light_color"
        app:layout_constraintTop_toBottomOf="@id/symbolName"
        app:layout_constraintStart_toStartOf="parent"
        android:text="LTP - 26530"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/symbolLtp"
        android:textSize="@dimen/small_text_size"
        android:layout_marginTop="@dimen/medium_margin"
        android:textColor="?attr/text_light_color"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:layout_marginStart="@dimen/medium_margin"
        app:layout_constraintTop_toBottomOf="@id/symbolName"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="LTP - 26530"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/symbolMarketType"
        android:textSize="@dimen/small_text_size"
        android:layout_marginTop="@dimen/medium_margin"
        android:textColor="?attr/text_light_color"
        android:fontFamily="@font/montserratregular"
        app:fontFamily="@font/montserratregular"
        android:layout_marginEnd="@dimen/medium_margin"
        android:layout_marginStart="@dimen/medium_margin"
        app:layout_constraintTop_toBottomOf="@id/symbolName"
        app:layout_constraintEnd_toStartOf="@id/symbolLtp"
        android:text="CMC LIMIT"/>


</androidx.constraintlayout.widget.ConstraintLayout>